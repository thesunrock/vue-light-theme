{
	"name": "Vue Light Theme",
	"type": "light",
	"colors": {
		"editor.background": "#f5f5f5",
		"editor.foreground": "#333333",

		"activityBar.background": "#f5f5f5",
		"activityBar.foreground": "#333333",
		"activityBar.inactiveForeground": "#309568",

		"sideBarSectionHeader.background": "#997fffda",
		"sideBarSectionHeader.foreground": "#f5f5f5",
		"minimap.errorHighlight": "#309568",

		"list.activeSelectionForeground": "#ffffff",
		"list.activeSelectionBackground": "#EAA67F",
	
		"badge.background": "#309568",
		"badge.foreground": "#ffffff",


		"editorIndentGuide.background": "#997fff2a",
		"editorIndentGuide.activeBackground": "#997fff5b",

		"titleBar.activeBackground": "#33495d",
		"titleBar.activeForeground": "#f5f5f5",
		"titleBar.inactiveBackground": "#33495de3",
		"titleBar.inactiveForeground": "#f5f5f5",

		"tab.inactiveForeground": "#fe5d908a",
		"tab.activeForeground": "#1960A2",

		"button.background": "#1960a2c4",

		"panel.border": "#A6C4F5",
		"menu.border": "#A6C4F5",

		"statusBar.background": "#309568"
	},
	"tokenColors": [
		{
			"scope": [
				"comment",
				
			],
			"settings": {
				"foreground": "#5b98a0bd",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				
				"keyword.other.phpdoc.php",		
			],
			"settings": {
				"foreground": "#007e58c9",
			}
		},
		{
			"scope": [
				"storage.type",
				"entity.other.attribute-name.class.css",
				"entity.name.type.class",
				"entity.other.attribute-name.html"

			],
			"settings": {
				"foreground": "#0077ff",
			}
		},
		{
			"scope": [
				
				"variable.scss",
				"entity.name.type.module.ts",
				"entity.name.type.alias.ts",
				"entity.name.type.ts",
				
				"keyword.operator.logical.php"
			],
			"settings": {
				"foreground": "#00529e"
			}
		},
		{
			"scope": [
				"string",
				
			],
			"settings": {
				"foreground": "#008046",
			}
		},
		{
			"scope": [
				
			
			],
			"settings": {
				"foreground": "#017742",
			}
		},
		{
			"scope": [
				"entity.name.tag.css",
				"entity.other.attribute-name.id.css",
			],
			"settings": {
				"foreground": "#91225bea",
			}
		},
		{
			"scope": [
				"keyword.control",
				"support.type.property-name.json",
				"keyword.other.using",
				"keyword.other.use",
				"keyword.other.namespace",
				"entity.name.tag.html",
				"support.function"
			],
			"settings": {
				"foreground": "#b425a8",
			}
		},
		{
			"scope": [
				"entity.name.function",
				"constant.numeric.css",
				"support.constant.property-value.css",
				
			],
			"settings": {
				"foreground": "#a55d06",
			}
		},
		{
			"scope": [
				"storage.modifier",
				"keyword.other.class.cs",
			],
			"settings": {
				"foreground": "#5e3de4"
			}
		},
	
		{
			"scope": [
				"keyword.control.at-rule.media.scss",
				"keyword.operator.logical.scss",
				"keyword.control.at-rule.media.scss",
				"entity.name.tag.reference.scss"
				
			],
			"settings": {
				"foreground": "#ff3273",
			}
		}
	]
}